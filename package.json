{
  "name": "minesweeper",
  "version": "1.0.0",
  "description": "\"Minesweeper Game\"",
  "main": "index.js",
  "scripts": {
    "webpack:dev": "npx webpack-cli --env dev",
    "webpack:prod": "npx webpack-cli --env prod",

    "tsc:dev":  "tsc --sourceRoot 'src' --sourceMap true",
    "tsc:prod": "tsc",

    "build:dev":  "npm run tsc:dev  && npm run webpack:dev",
    "build:prod": "npm run tsc:prod && npm run webpack:prod",

    "build:clean": "rm -r build && npm run build:prod",

    "test":  "npm run build:dev   && node ./build/app.js",
    "start": "npm run build:clean && node ./build/app.js"
  }, 
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.3",
    "@types/socket.io": "^3.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.0.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
